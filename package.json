{
  "name": "saboteur-ts",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.11",
  "volta": {
    "node": "20.1.0"
  }
}
