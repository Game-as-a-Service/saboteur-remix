{
  "name": "backend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "nest build",
    "format": "prettier --no-error-on-unmatched-pattern --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.3.12",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/websockets": "^9.3.12",
    "@sindresorhus/is": "^5.3.0",
    "@socket.io/redis-streams-adapter": "^0.1.0",
    "fp-ts": "^2.13.1",
    "neverthrow": "^6.0.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.1",
    "ts-pattern": "^4.2.2",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "dotenv-flow": "^3.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "mock-fs": "^5.2.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=14.16"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules"
  ]
}
